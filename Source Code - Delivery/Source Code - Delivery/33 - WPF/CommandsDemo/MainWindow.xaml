<Window x:Class="CommandsDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="183" Width="342" Loaded="Window_Loaded">
    <Grid>
        <Menu Height="23" Name="menu1" VerticalAlignment="Top">
            <MenuItem Header="File">
                <MenuItem Command="ApplicationCommands.New" />
            </MenuItem>
        </Menu>
        <Button x:Uid="button_1" Height="23" HorizontalAlignment="Left" Margin="54,60,0,0" Name="button1" VerticalAlignment="Top" Width="75" Content="Button" Command="ApplicationCommands.New" />
        <Button Height="23" HorizontalAlignment="Left" Margin="163,60,0,0" Name="button2" VerticalAlignment="Top" Width="75" Command="ApplicationCommands.New" Content="Button" />
        <CheckBox Content="Command Active" Height="16" HorizontalAlignment="Left" Margin="99,100,0,0" Name="checkBox1" VerticalAlignment="Top" IsChecked="True" />
    </Grid>
</Window>
